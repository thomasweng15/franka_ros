<?xml version="1.0" ?>
<launch>
  <arg name="robot_id" />
  <arg name="robot_ip" />
  <arg name="load_gripper" default="true" />

  <arg name="robot" default="$(find franka_description)/robots/dual_panda_example.urdf.xacro" />
  <param name="robot_description" command="xacro $(arg robot)" />

  <include file="$(find franka_gripper)/launch/franka_gripper.launch" if="$(arg load_gripper)">
    <!-- <arg name="robot_id" value="$(arg robot_id)" /> -->
    <arg name="robot_ip" value="$(arg robot_ip)" />
  </include>

  <node name="franka_control_$(arg robot_id)" pkg="franka_control" type="franka_control_node" output="screen" required="true">
    <rosparam command="load" file="$(find franka_control)/config/franka_control_node_dual_$(arg robot_id).yaml" />
    <param name="robot_ip" value="$(arg robot_ip)" />
  </node>

  <rosparam command="load" file="$(find franka_control)/config/default_controllers_dual_$(arg robot_id).yaml" />
  <node name="state_controller_spawner_$(arg robot_id)" pkg="controller_manager" type="spawner" respawn="false" output="screen" 
    args="panda_$(arg robot_id)_state_controller position_$(arg robot_id)_joint_trajectory_controller">
  </node>

  <node name="robot_state_publisher_$(arg robot_id)" pkg="robot_state_publisher" type="robot_state_publisher" output="screen"/>
  <!-- <node name="joint_state_publisher_$(arg robot_id)" type="joint_state_publisher" pkg="joint_state_publisher" output="screen">
    <rosparam if="$(arg load_gripper)" param="source_list">[panda_2_state_controller/joint_states, franka_gripper/joint_states] </rosparam>
    <rosparam unless="$(arg load_gripper)" param="source_list">[panda_2_state_controller/joint_states] </rosparam>
    <param name="rate" value="30"/>
  </node>
  <node name="joint_state_desired_publisher_$(arg robot_id)" type="joint_state_publisher" pkg="joint_state_publisher" output="screen">
    <rosparam if="$(arg load_gripper)" param="source_list">[panda_2_state_controller/joint_states_desired, franka_gripper/joint_states] </rosparam>
    <rosparam unless="$(arg load_gripper)" param="source_list">[panda_2_state_controller/joint_states_desired] </rosparam>
    <param name="rate" value="30"/>
    <remap from="/joint_states" to="/joint_states_desired" />
  </node> -->
</launch>
